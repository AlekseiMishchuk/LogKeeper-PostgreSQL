@rendermode InteractiveServer
@page "/"
@page "/loglist/{CurrentPage:int}"

<CascadingValue Value="@CurrentPage">
    <LogList/>
</CascadingValue>


@code
{
    [Parameter]
    public int CurrentPage { get; set; } = 1;
    
    protected override void OnInitialized()
    {
        if (CurrentPage <= 1)
        {
            CurrentPage = 1;
        }
    }
}
